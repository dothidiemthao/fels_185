<%= provide :title, t("admin.word.title") %>
<%= provide :submit_button, t("admin.word.create_word") %>
<div class="row" id="word">
  <div class="col-lg-12">
    <div class="col-lg-offset-2 col-md-8">
      <h2><b><%= t "admin.word.create_word" %></b></h2>
      <%= form_for [:admin, :category, @word] do |f| %>
        <%= render "form", f: f %>
      <% end %>
    </div>
  </div>
</div>
